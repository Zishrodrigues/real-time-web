<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">
    <h1>Zish's chat app</h1>
    <form id='userForm' class="userForm">
        <label>Please enter a nickname</label>
        <input id='username' />
        <input type='submit' class='btn btn-primary' value='Chat!' />
    </form>
    <h5 id='onlineUsers'></h5>
    <ul id="currentUsers"></ul>
    <section id="mainImg" class="imageWrapper hide">
        <img id="image" class="mainImage zoomed" src="" alt="Image">
    </section>
    <ul id="messages" class="messageList hide"></ul>
    <form action="" id="chatForm" class="chatForm hide">
        <input id="m" autocomplete="off" /><button>Send</button>
    </form>
    <!-- <script src="http://localhost:3007/socket.io/socket.io.js"></script> -->
    <script src="https://cdn.socket.io/socket.io-1.2.0.js"></script>
    <script src="/js/script.js"></script>
</body>
</html>
